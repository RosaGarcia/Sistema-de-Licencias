<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('fabricante_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.razonSocial }}</td>
            <td>{{ entity.rfc }}</td>
            <td>{{ entity.telefono1 }}</td>
            <td>{{ entity.ext1 }}</td>
            <td>{{ entity.telefono2 }}</td>
            <td>{{ entity.ext2 }}</td>
            <td>{{ entity.email }}</td>
            <td>{{ entity.paginaWeb }}</td>
            <td>
                <a href="{{ path('fabricante_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>

                <a href="{{ path('direccionfabricante_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">Editar</a>                  
            </td>
        </tr>
    {% endfor %}
</tbody>