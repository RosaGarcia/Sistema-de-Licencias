<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto">{{ entity.id }}</td>
            <td><a href="{{ path('usuarios_show', { 'id': entity.id }) }}">{{ entity.userName }}</a></td>
            <td>{{ entity.personal }}</td>
            <td>{{ entity.rol }}</td>
            <td>
                <a href="{{ path('usuarios_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>
                                
                <a href="{{ path('usuarios_edit', { 'id': entity.id }) }}"  class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>  