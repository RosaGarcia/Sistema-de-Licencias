<?php

namespace Crm\EquipoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CaracteristicasEquipo
 */
class CaracteristicasEquipo
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $interfaz;

    /**
     * @var integer
     */
    private $usb;

    /**
     * @var boolean
     */
    private $expansion;

    /**
    * @var string
    */
    private $capacidad;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set interfaz
     *
     * @param string $interfaz
     * @return CaracteristicasEquipo
     */
    public function setInterfaz($interfaz)
    {
        $this->interfaz = $interfaz;

        return $this;
    }

    /**
     * Get interfaz
     *
     * @return string 
     */
    public function getInterfaz()
    {
        return $this->interfaz;
    }

    /**
     * Set usb
     *
     * @param integer $usb
     * @return CaracteristicasEquipo
     */
    public function setUsb($usb)
    {
        $this->usb = $usb;

        return $this;
    }

    /**
     * Get usb
     *
     * @return integer 
     */
    public function getUsb()
    {
        return $this->usb;
    }

    /**
     * Set expansion
     *
     * @param boolean $expansion
     * @return CaracteristicasEquipo
     */
    public function setExpansion($expansion)
    {
        $this->expansion = $expansion;

        return $this;
    }

    /**
     * Get expansion
     *
     * @return boolean 
     */
    public function getExpansion()
    {
        return $this->expansion;
    }

    /**
     * Set capacidad
     *
     * @param string $capacidad
     * @return CaracteristicasEquipo
     */
    public function setCapacidad($capacidad)
    {
        $this->capacidad = $capacidad;

        return $this;
    }

    /**
     * Get capacidad
     *
     * @return string 
     */
    public function getCapacidad()
    {
        return $this->capacidad;
    }
    /**
     * @var \Crm\EquipoBundle\Entity\Equipo
     */
    private $equipo;

    /**
     * @var \Crm\CaracteristicasBundle\Entity\Procesador
     */
    private $procesador;

    /**
     * @var \Crm\CaracteristicasBundle\Entity\MemoriaRam
     */
    private $memoriaRam;

    /**
     * @var \Crm\CaracteristicasBundle\Entity\DiscoDuro
     */
    private $discoDuro;

    /**
     * @var \Crm\CaracteristicasBundle\Entity\VersionSoftware
     */
    private $software;

    /**
     * @var \Crm\CaracteristicasBundle\Entity\Dimenciones
     */
    private $dimenciones;


    /**
     * Set equipo
     *
     * @param \Crm\EquipoBundle\Entity\Equipo $equipo
     * @return CaracteristicasEquipo
     */
    public function setEquipo(\Crm\EquipoBundle\Entity\Equipo $equipo = null)
    {
        $this->equipo = $equipo;

        return $this;
    }

    /**
     * Get equipo
     *
     * @return \Crm\EquipoBundle\Entity\Equipo 
     */
    public function getEquipo()
    {
        return $this->equipo;
    }

    /**
     * Set procesador
     *
     * @param \Crm\CaracteristicasBundle\Entity\Procesador $procesador
     * @return CaracteristicasEquipo
     */
    public function setProcesador(\Crm\CaracteristicasBundle\Entity\Procesador $procesador = null)
    {
        $this->procesador = $procesador;

        return $this;
    }

    /**
     * Get procesador
     *
     * @return \Crm\CaracteristicasBundle\Entity\Procesador 
     */
    public function getProcesador()
    {
        return $this->procesador;
    }

    /**
     * Set memoriaRam
     *
     * @param \Crm\CaracteristicasBundle\Entity\MemoriaRam $memoriaRam
     * @return CaracteristicasEquipo
     */
    public function setMemoriaRam(\Crm\CaracteristicasBundle\Entity\MemoriaRam $memoriaRam = null)
    {
        $this->memoriaRam = $memoriaRam;

        return $this;
    }

    /**
     * Get memoriaRam
     *
     * @return \Crm\CaracteristicasBundle\Entity\MemoriaRam 
     */
    public function getMemoriaRam()
    {
        return $this->memoriaRam;
    }

    /**
     * Set discoDuro
     *
     * @param \Crm\CaracteristicasBundle\Entity\DiscoDuro $discoDuro
     * @return CaracteristicasEquipo
     */
    public function setDiscoDuro(\Crm\CaracteristicasBundle\Entity\DiscoDuro $discoDuro = null)
    {
        $this->discoDuro = $discoDuro;

        return $this;
    }

    /**
     * Get discoDuro
     *
     * @return \Crm\CaracteristicasBundle\Entity\DiscoDuro 
     */
    public function getDiscoDuro()
    {
        return $this->discoDuro;
    }

    /**
     * Set software
     *
     * @param \Crm\CaracteristicasBundle\Entity\VersionSoftware $software
     * @return CaracteristicasEquipo
     */
    public function setSoftware(\Crm\CaracteristicasBundle\Entity\VersionSoftware $software = null)
    {
        $this->software = $software;

        return $this;
    }

    /**
     * Get software
     *
     * @return \Crm\CaracteristicasBundle\Entity\VersionSoftware 
     */
    public function getSoftware()
    {
        return $this->software;
    }

    /**
     * Set dimenciones
     *
     * @param \Crm\CaracteristicasBundle\Entity\Dimenciones $dimenciones
     * @return CaracteristicasEquipo
     */
    public function setDimenciones(\Crm\CaracteristicasBundle\Entity\Dimenciones $dimenciones = null)
    {
        $this->dimenciones = $dimenciones;

        return $this;
    }

    /**
     * Get dimenciones
     *
     * @return \Crm\CaracteristicasBundle\Entity\Dimenciones 
     */
    public function getDimenciones()
    {
        return $this->dimenciones;
    }
}
