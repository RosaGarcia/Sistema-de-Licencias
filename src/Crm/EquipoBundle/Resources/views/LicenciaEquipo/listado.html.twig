<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('licenciaequipo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.equipo }}</td>
            <td>{% if entity.fechaInicio %}{{ entity.fechaInicio|date('Y-m-d') }}{% endif %}</td>
            <td>{% if entity.fechaFinal %}{{ entity.fechaFinal|date('Y-m-d') }}{% endif %}</td>
            <td>
                <a href="{{ path('licenciaequipo_show', { 'id': entity.id }) }}"  class="btn yellow btn-outline">Ver</a>
                                        
                <a href="{{ path('licenciaequipo_edit', { 'id': entity.id }) }}"  class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>