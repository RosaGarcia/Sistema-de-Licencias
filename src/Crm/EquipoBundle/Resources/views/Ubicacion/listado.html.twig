<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('ubicacion_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.calle }}</td>
            <td>{{ entity.numeroExt }}</td>
            <td>{{ entity.numeroInt }}</td>
            <td>{{ entity.colonia }}</td>
            <td>{{ entity.codigoPostal }}</td>
            <td>
                <a href="{{ path('ubicacion_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>
                                        
                <a href="{{ path('ubicacion_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>