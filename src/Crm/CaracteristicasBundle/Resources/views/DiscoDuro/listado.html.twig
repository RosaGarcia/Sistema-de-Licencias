<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('discoduro_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.marca }}</td>
            <td>{{ entity.capacidad }} {{ entity.unidad }}</td>
            <td>{{ entity.tipo }}</td>
            <td>{{ entity.terminoDisco }}</td>
            <td>
                <a href="{{ path('discoduro_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>
                                            
                <a href="{{ path('discoduro_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>