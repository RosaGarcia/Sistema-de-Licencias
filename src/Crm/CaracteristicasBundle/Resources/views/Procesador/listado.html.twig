<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('procesador_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.marca }}</td>
            <td>{{ entity.modelo }}</td>
            <td>{{ entity.velocidad }} {{ entity.unidad }}</td>
            <td>{{ entity.terminoProcesador }}</td>
            <td>
                <a href="{{ path('procesador_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>
                                    
                <a href="{{ path('procesador_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>