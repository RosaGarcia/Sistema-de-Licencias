{% extends '::frontend.html.twig' %}
{% block body %}
<div class="row">
        <div class="col-xs-12 col-sm-12 com-md-12">
            <h2>
                Dimensiones
            </h2>
        </div>
    </div>

<div class="row">
    <div class="col-md-12 col-sm-12">
        <div class="portlet light ">
            <div class="portlet-title tabbable-line">
                <div class="caption">
                    <i class="fa fa-database font-green-sharp"></i>
                    <span class="caption-subject font-green-sharp bold uppercase">Nueva Dimension</span>
                </div>
                <ul class="nav nav-tabs">
                    <li class="active">
                        <a href="#tab_1_1" class="active" data-toggle="tab"> Dimensi√≥n </a>
                    </li>
                </ul>
            </div>
            {{ form_start(form, { 'attr' : {'novalidate': 'novalidate', 'role' : 'form'} }) }}
                <div class="portlet-body">
                    <div class="tab-content">
                        <div class="tab-pane active">
                            <div class="scroller" data-always-visible="1" data-rail-visible="0">
                                

                                    <div class="col-md-6">
                                        <div class="form-group form-md-line-input has-success">
                                            <div class="input-icon">
                                                {{ form_widget(form.dimensiones) }}
                                                <span class="help-block help-block-error">{{ form_errors(form.dimensiones) }}</span>
                                                <label for="dimensiones">Dimensiones</label>
                                                <i class="fa fa-pencil"></i>
                                            </div>
                                        </div>
                                    </div>
                    
                               
            {{ form_widget(form._token) }}  
                         
                                <div class="col-xs-12"><br /><br />
                                    <button class="btn btn-success" type="submit" id="sendform"><i class="fa fa-plus"></i>&nbsp;Crear</button>
                                </div>
                          
                                </form>     
                            </div>
                        </div>
                                            
                    </div>
                </div>
        </div>
    </div>
</div>
{% endblock %}
