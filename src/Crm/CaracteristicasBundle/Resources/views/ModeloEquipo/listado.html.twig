<tbody>
    {% for entity in entities %}
        <tr>
            <td class="oculto"><a href="{{ path('modeloequipo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nombre }}</td>
            <td>
                <a href="{{ path('modeloequipo_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">Ver</a>

                <a href="{{ path('modeloequipo_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">Editar</a>
            </td>
        </tr>
    {% endfor %}
</tbody>